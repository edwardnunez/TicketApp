{
    "name": "gatewayservice",
    "version": "1.0.0",
    "main": "gateway-service.js",
    "type": "module",
    "scripts": {
      "start": "node gateway-service.js",
      "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
    },
    "dependencies": {
      "axios": "^1.6.7",
      "cors": "^2.8.5",
      "express": "^4.18.2"
    },
    "devDependencies": {
      "cross-env": "^10.1.0",
      "jest": "^29.7.0",
      "supertest": "^6.3.3",
      "@types/jest": "^29.5.5"
    },
    "jest": {
      "testEnvironment": "node",
      "testMatch": ["**/__tests__/**/*.test.js"],
      "collectCoverageFrom": [
        "**/*.js",
        "!**/node_modules/**",
        "!**/__tests__/**"
      ],
      "coverageDirectory": "coverage",
      "coverageReporters": ["text", "lcov", "html"],
      "transform": {}
    }
  }
  