{
    "name": "gatewayservice",
    "version": "1.0.0",
    "main": "gateway-service.js",
    "type": "module",
    "scripts": {
        "start": "node gateway-service.js",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
        "lint": "eslint *.js --max-warnings 0"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "cors": "^2.8.5",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.5",
        "cross-env": "^10.1.0",
        "eslint": "^9.38.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.3"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.js"
        ],
        "collectCoverageFrom": [
            "**/*.js",
            "!**/node_modules/**",
            "!**/__tests__/**"
        ],
        "coverageDirectory": "coverage",
        "coverageReporters": [
            "text",
            "lcov",
            "html"
        ],
        "transform": {}
    }
}
